{
    "name": "Input Logger",
    "version": "1.0",
    "description": "Logs user inputs from text fields and text areas.",
    "manifest_version": 3,
    "permissions": ["activeTab"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon.png",
        "48": "images/icon.png",
        "128": "images/icon.png"
      }
    }
  }
  